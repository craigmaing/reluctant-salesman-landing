{
  "name": "reluctant-salesman",
  "static_sites": [
    {
      "name": "web",
      "github": {
        "repo": "craigmaing/reluctant-salesman-landing",
        "branch": "master",
        "deploy_on_push": true
      },
      "build_command": "npm run build",
      "output_dir": "dist",
      "environment_slug": "node-js",
      "envs": [
        {
          "key": "RESEND_API_KEY",
          "value": "",
          "scope": "BUILD_TIME",
          "type": "SECRET"
        },
        {
          "key": "PUBLIC_SITE_URL",
          "value": "https://thereluctantsalesman.com",
          "scope": "BUILD_TIME",
          "type": "GENERAL"
        }
      ],
      "routes": [
        {
          "path": "/"
        }
      ]
    }
  ],
  "region": "lon"
}